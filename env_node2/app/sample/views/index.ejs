<!DOCTYPE html>
<html lang="ja">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Document</title>
</head>
<body>
  <table border="1">
    <% books.forEach(function (book, key) { %>
    <tr>
      <td><%= key %></td>
      <td><%= book.title %></td>
      <td><%= book.date %></td>
    </tr>
    <% }); %>
  </table>
</body>
</html>
